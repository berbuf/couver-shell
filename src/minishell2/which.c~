#include "mo.h"

void	my_which_where(char **tab, t_shell *sh)
{
  char	*str_path;
  char	**tab_path;
  int	n;

  n = -1
  if ((str_path = get_var_env(sh->env, "PATH=")) == NULL)
    {
      n = confstr(_CS_PATH, NULL, (size_t) 0);
      str_path = my_malloc(sizeof(char) * (n + 1));
      confstr(_CS_PATH, str_path, n);
    }
}
